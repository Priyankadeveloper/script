<script stype="text/javascript">
  jQuery(document).ready(function()
  {
    jQuery("#Flat-Sizes-list").click(function(){
        //alert("The paragraph was clicked.");
        jQuery('html, body').animate(
          {
               scrollTop: jQuery("#Flat-Sizes").offset().top
          }, 2000);
    });
  });

